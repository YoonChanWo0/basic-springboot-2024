<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cw98.spring02.mapper.TodoMapper">

    <select id="selectTods" resultType="hashmap">
        SELECT TNO
	        , TITLE 
	        , DUEDATE 
	        , WRITER 
	        , ISDONE
        FROM TODOS;
    </select>

    <select>
